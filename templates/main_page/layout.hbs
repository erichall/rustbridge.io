<!doctype html>
<html>
  <head>
    <title>{{title}}</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
    <link rel="stylesheet" href="/static/styles.css"/>
    <link rel="icon" href="/favicon.ico" type="image/x-icon">

    <meta name="twitter:card" content="summary">
    <meta name="twitter:url" content="rustbridge.github.io">
    <meta name="twitter:title" content="RustBridge - We're here to teach you Rust">
    <meta name="twitter:description" content="RustBridge is a workshop focused on getting underrepresented people with a background in another programming language to learn Rust and join the community.">
    <meta name="twitter:image" content="https://www.rust-lang.org/logos/rust-logo-blk.svg"/>

  </head>
  <body>
    {{> main_page/nav }}
    {{> main_page/header }}
    <div class="container">
      <div class="row">
        <div class="col-md-8" id="about">
          {{> page }}
        </div>
        <div class="col-md-4 well well-lg" id="workshops">
          {{~> (sidebar)~}}
        </div>
      </div>
    </div>
    {{> main_page/footer }}
    {{> main_page/scripts }}
  </body>
</html>

